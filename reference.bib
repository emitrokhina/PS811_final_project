@article{rozenasPersuasionPropagandaExperimental,
  title = {Persuasion {{Through Propaganda}}: {{Experimental Evidence}} from {{Russia}}},
  author = {Rozenas, Arturas and Stukal, Denis and Syunyaev, Georgiy},
  pages = {45},
  abstract = {When is propaganda persuasive and whose beliefs can it affect the most and the least? To address this question, we propose a simple model of belief-updating in which citizens are uncertain about the competence of their government, but they are also uncertain about the bias of the media source, which tells them that the government is competent. Upon observing propaganda messages, citizens who are a priori skeptical about the honesty of the media source become even more convinced that the media is biased and do not update their beliefs about the competence of the government. In contrast, citizens who are a priori skeptical about the government’s competence but are not skeptical about the media are most liable to update their beliefs in favor of the government. As a result, propaganda not only fails to persuade those who are a priori skeptical about the honesty of the state media, but it also makes them less likely to be persuaded in the future. The model allows us to identify the types of citizens who are most and least susceptible to being persuaded by propaganda, and allows us to draw structured predictions about the effects of propaganda on the citizens’ beliefs about both the content of the messages as well as the biasedness of the media. We also present the preliminary results from an online panel experiment, which tests the main predictions of the model for the effects of media reporting on evaluation of the economy, leader’s competence and media bias on the sample of Russian citizens. The experiment includes two main treatment groups where respondents watched excerpts from the main Russian TV channel, Channel 1, regarding the state of economy and regarding the state of economy mentioning the president of the country respectively. We compare the change of the evaluations in those treatment groups to two control groups: pure control, where respondents were not offered to watch any news reports, and placebo control, where respondents were offered a video excerpt not talking about the state of economy.},
  file = {C\:\\Users\\user\\Zotero\\storage\\JNWSIBHP\\Rozenas и др. - Persuasion Through Propaganda Experimental Eviden.pdf},
