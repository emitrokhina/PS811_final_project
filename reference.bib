@article{mamonovaUnderstandingSilentMajority2019,
  title = {Understanding the Silent Majority in Authoritarian Populism: What Can We Learn from Popular Support for {{Putin}} in Rural {{Russia}}?},
  shorttitle = {Understanding the Silent Majority in Authoritarian Populism},
  author = {Mamonova, Natalia},
  date = {2019-04-16},
  journaltitle = {The Journal of Peasant Studies},
  volume = {46},
